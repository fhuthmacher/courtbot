{
    "prod": {
        "app_function": "courtbot.app",
        "aws_region": "us-east-1",
        // TODO: Use this to prune old Lambda versions.
        // https://github.com/Miserlou/Zappa/issues/254
        // "callbacks": {
        //     "post": "courtbot.post_callback",
        // },
        "exclude": [
            ".flake8",
            ".git/*",
            ".gitignore",
            ".python-version",
            ".travis.yml",
            "LICENSE",
            "Makefile",
            "README.md",
            "requirements.txt",
            "zappa_settings.json",
        ],
        "keep_warm": true,
        "keep_warm_expression": "rate(5 minutes)",
        "memory_size": 128,
        "profile_name": "default",
        "project_name": "courtbot",
        "runtime": "python3.6",
        "s3_bucket": "courtbot",
        "timeout_seconds": 30,
    }
}
